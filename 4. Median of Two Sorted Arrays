def findMedianSortedArrays(nums1: List[int], nums2: List[int]) -> float:
        nums=nums1+nums2
        nums.sort()
        
        size=len(nums)
        
        if len(nums)%2==0:
            
            return (nums[size//2]+nums[size//2+1])/2.0
        else:
            return nums[size//2]
       
print([1,3],[2])
print([1,2],[3,4])
print([0,0],[0,0])
print([],[1])
print([2],[])    

        
        
        
 
